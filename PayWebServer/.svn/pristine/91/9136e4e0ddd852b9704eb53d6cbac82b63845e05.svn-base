#if($sqlName=='queryMerEnableMoneyByDay')
SELECT 
       SUM(wx_pay_money) AS PAY_MONEY, 
       SUM(wx_refund_money) AS REFUND_MONEY, 
       SUM(wx_revoke_money) AS REVOKE_MONEY,
       SUM(ali_pay_money) AS ALI_PAY_MONEY, 
       SUM(ali_refund_money) AS ALI_REFUND_MONEY, 
       SUM(ali_revoke_money) AS ALI_REVOKE_MONEY,
       SUM(wx_pay_money) - SUM(wx_refund_money) - SUM(wx_revoke_money) as WX_ENABLE_MONEY,
       SUM(ali_pay_money) - SUM(ali_refund_money) - SUM(ali_revoke_money) as ALI_ENABLE_MONEY
FROM (
	SELECT CASE 
			WHEN t.txn_sta = '02'
			AND t.txn_type = '01' 
			AND t.PAY_ACCESS_TYPE = '02'
			THEN t.trade_money
			ELSE 0
		END AS wx_pay_money
		, CASE 
			WHEN t.txn_sta = '02'
			AND t.txn_type = '01' 
			AND t.PAY_ACCESS_TYPE = '03'
			THEN t.trade_money
			ELSE 0
		END AS ali_pay_money
		,CASE 
			WHEN t.txn_sta != '03'
			AND t.txn_type = '34' 
			AND t.PAY_ACCESS_TYPE = '02'
			THEN t.trade_money
			ELSE 0
		END AS wx_refund_money
		,CASE 
			WHEN t.txn_sta != '03'
			AND t.txn_type = '34' 
			AND t.PAY_ACCESS_TYPE = '03'
			THEN t.trade_money
			ELSE 0
		END AS ali_refund_money
		, CASE 
			WHEN t.txn_sta != '03'
			AND t.txn_type = '31' 
			AND t.PAY_ACCESS_TYPE = '02'
			THEN t.trade_money
			ELSE 0
		END AS wx_revoke_money
		, CASE 
			WHEN t.txn_sta != '03'
			AND t.txn_type = '31' 
			AND t.PAY_ACCESS_TYPE = '03'
			THEN t.trade_money
			ELSE 0
		END AS ali_revoke_money
	FROM tbl_order_txn t
	WHERE t.MER_ID = '${merId}'
	  AND t.TXN_DT = '${txnDt}'
)
#end

